{
    "name": "akropolis-delphi-yearn",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://github.com/akropolisio/delphi-yearn.git",
    "author": "Pavel Rubin <pash7ka@gmail.com>",
    "license": "MIT",
    "scripts": {
        "compile": "npx brownie compile --all",
        "ganache": "npx ganache-cli --gasLimit 0xfffffffffff -e 10000000 --port 8545",
        "generate-types": "typechain --target=truffle-v5 \"build/contracts/*.json\"",
        "generate-abi": "npx truffle-abi -o ./abi",
        "generate-artifacts": "npm run compile && npm run generate-abi && npm run generate-types",
        "install": "pip3 install -r requirements-dev.txt --no-index",
        "postinstall": "npm run generate-artifacts",
        "test": "npx brownie test"
    },
    "dependencies": {
        "@openzeppelin/cli": "^2.8.2",
        "@types/bn.js": "^4.11.6",
        "@types/chai": "^4.2.11",
        "@types/mocha": "^8.0.0",
        "@types/web3": "^1.2.2",
        "dotenv": "^8.2.0",
        "solc": "^0.6.12",
        "truffle-assertions": "^0.9.2"
    },
    "devDependencies": {
        "@truffle/hdwallet-provider": "^1.0.38",
        "@typechain/truffle-v5": "^2.0.2",
        "ts-node": "^8.10.2",
        "typechain": "^2.0.0",
        "typescript": "^3.9.7"
    }
}
